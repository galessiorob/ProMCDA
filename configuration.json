{
 "input_matrix_path": "/path/to/input_matrix.csv",
  "polarity_for_each_indicator": ["+","-","+","+","+","-","+","+"],

  "sensitivity": {
    "sensitivity_on": "no",
    "normalization": "minmax",
    "aggregation": "geometric"},

  "robustness": {
    "robustness_on": "yes",
    "on_single_weights": "no",
    "on_all_weights": "no",
    "given_weights": [0.5, 0.2, 0.8, 0.5, 0.5, 0.9, 0.1, 0.5],
    "on_indicators": "yes"},

  "monte_carlo_sampling": {
      "monte_carlo_runs": 10000,
      "num_cores": 1,
      "marginal_distribution_for_each_indicator": ["normal", "lnorm", "poisson", "uniform", "exact", "normal", "normal", "normal"]},

  "output_path": "/path/to/output/directory"
}